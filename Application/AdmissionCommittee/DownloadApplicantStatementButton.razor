@inject NavigationManager NavigationManager
<MudIconButton Icon="@Icons.Filled.Print" Title="Скачать заявление" OnClick="@OnClick"/>

@code {

    [Parameter]
    public long ApplicantId { get; set; }

    private void OnClick()
    {
        NavigationManager.NavigateTo($"applicants/{ApplicantId}/statement", true);
    }

}